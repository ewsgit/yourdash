{
  "name": "yourdash",
  "version": "0.0.1",
  "repository": "git@github.com:yourdash-app/yourdash",
  "author": "ewsgit <ewsgit@duck.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "yarn": ">=1.22.0"
  },
  "workspaces": [
    "packages/*",
    "main/*",
    "applications"
  ],
  "scripts": {
    "yd:dev-web": "yarn --cwd main/web dev",
    "yd:dev-deprecated-web": "yarn --cwd ./main/deprecated-web dev",
    "yd:dev-backend": "yarn --cwd ./main/backend dev",
    "yd:start-backend": "yarn --cwd ./main/backend start --log-requests",
    "yd:setup-project": "yarn && yarn --cwd main/web generate-application-routes",
    "yd:build-production": "yarn --cwd ./main/backend compile-all && yarn --cwd ./packages/web-client build",
    "opengfx:dev": "yarn --cwd ./packages/opengfx-engine dev",
    "yd:dev-desktop-client": "yarn --cwd ./main/desktop-client/ dev"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "bufferutil": "^4.0.8",
    "discord.js": "^14.14.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "run-script-os": "^1.1.6",
    "typescript-eslint": "^0.0.1-alpha.0",
    "utf-8-validate": "^6.0.3",
    "yarn": "^1.22.21",
    "zlib-sync": "^0.1.9"
  }
}
