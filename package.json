{
  "name": "yourdash",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "projects/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "dev:main": "concurrently \"yarn --cwd ./projects/web dev\" \"yarn --cwd ./projects/server dev\"",
    "build:main": "yarn --cwd ./projects/web build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
