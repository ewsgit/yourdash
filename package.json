{
  "name": "yourdash",
  "version": "0.0.0",
  "description": "The YourDash project",
  "main": "index.js",
  "repository": "https://github.com/ewsgit/yourdash",
  "author": "Ewsgit",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start-client": "yarn --cwd ./client dev",
    "visualize-client-bundle": "yarn --cwd ./client visualize-bundle",
