{
  "name": "yourdash",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev:all": "turbo run dev",
    "dev:chipletui": "yarn --cwd ./apps/chipletui-project dev",
    "dev": "concurrently \"yarn --cwd ./apps/server dev\" \"yarn --cwd ./apps/web dev\"",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
