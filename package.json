{
  "name": "yourdash",
  "version": "0.0.1",
  "repository": "git@github.com:yourdash-app/yourdash",
  "author": "ewsgit <ewsgit@duck.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "web": "bun run --cwd=packages/web dev",
    "backend": "bun run --cwd=packages/backend dev",
    "build-deps": "bun run --cwd=packages/uikit build && bun run --cwd=packages/tunnel build",
    "legacy-backend": "bun run --cwd=packages/backend legacy-dev",
    "production-backend": "bun run --cwd=packages/backend production",
    "postinstall": "bun run build-deps"
  },
  "dependencies": {
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "sharp": "^0.33.5"
  }
}
