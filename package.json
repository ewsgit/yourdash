{
  "name": "yourdash",
  "version": "0.0.0",
  "description": "The YourDash project",
  "main": "index.js",
  "repository": "https://github.com/ewsgit/yourdash",
  "author": "Ewsgit",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start-client": "yarn --cwd ./client dev",
    "visualize-client-bundle": "yarn --cwd ./client visualize-bundle",
    "start-server": "yarn --cwd ./server start",
    "start-server-dev": "concurrently \"yarn --cwd ./server dev\" \"yarn --cwd ./server compile\"",
    "start-desktop": "concurrently \"tsc --watch\" \"cross-env DEV=true electron ./electron/index.js\" \"yarn run start-client\" \"yarn run start-server-dev\"",
    "postinstall": "yarn --cwd ./client && yarn --cwd ./server"
  },
  "dependencies": {
    "@types/node": "^20.1.4",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.3.0",
    "express": "^4.18.2",
    "path": "^0.12.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "yarn": "^1.22.19"
  }
}
