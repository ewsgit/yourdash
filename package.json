{
  "name": "yourdash",
  "scripts": {
    "start-backend": "yarn --cwd ./app/backend start",
    "compile-backend": "yarn --cwd ./app/backend typescript",
    "sync-applications": "yarn --cwd ./app/applications start",
    "start-desktop": "yarn --cwd ./app/electron start",
    "start-frontend": "yarn --cwd ./app/frontend start",
    "postinstall": "concurrently \"yarn --cwd ./app/backend\" \"yarn --cwd ./app/electron\" \"yarn --cwd ./app/frontend\" \"yarn --cwd ./app/applications\" && echo All packages installed!",
    "application-manager": ""
  },
  "packageManager": "yarn@1.22.0+",
  "version": "0.0.0",
  "description": "The Yourdash Project",
  "main": "index.js",
  "repository": "https://github.com/ewsgit/yourdash",
  "author": "ewsgit",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
