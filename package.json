{
  "name": "yourdash",
  "version": "0.0.1",
  "repository": "git@github.com:yourdash-app/yourdash",
  "author": "ewsgit <ewsgit@duck.com>",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "electron"
    ]
  },
  "scripts": {
    "yourdash:dev-web-client": "yarn --cwd ./packages/web-client dev",
    "yourdash:dev-backend": "yarn --cwd ./packages/backend dev --log-requests",
    "yourdash:setup-project": "yarn && yarn --cwd ./packages/backend compile && yarn --cwd ./packages/backend compile-all",
    "yourdash:build-production": "yarn --cwd ./packages/backend compile-all && yarn --cwd ./packages/web-client build",
    "opengfx:dev": "yarn --cwd ./packages/opengfx-engine dev",
    "yourdash:dev-desktop-client": "yarn --cwd ./packages/desktop-client/ dev",
    "yourdash:compile-backend": "yarn --cwd ./packages/backend compile"
  },
  "dependencies": {}
}
